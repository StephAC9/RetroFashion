ext: 'Summer', value: 'Summer' },\n         { text: 'Spring', value: 'Spring' },\n         { text: 'Autumn', value: 'Autumn' },\n     ],                \n     images:[]      \n    }\n  },\n  methods: {\n    onFileChange1(e) {\n      var files = e.target.files || e.dataTransfer.files;\n      console.log(files)\n      if (!files.length)\n        return;\n      this.createImage1(files[0]);\n    },\n     createImage1(file) {\n       console.log(file)\n      var reader = new FileReader()\n\n      reader.onload = (e) => {\n        this.image1 = e.target.result\n        this.mainImage = this.e.target.result\n        this.images.push(this.image1)\n        console.log('Images:   ',this.images)\n        }\n        \n      reader.readAsDataURL(file);\n    }, \n    removeImage1: function () {\n      this.image2 = '';\n    },\n\n\n     onFileChange2(e) {\n      var files = e.target.files || e.dataTransfer.files;\n      console.log(files)\n      if (!files.length)\n        return;\n      this.createImage2(files[0]);\n    },\n     createImage2(file) {\n       console.log(file)\n      var reader = new FileReader()\n\n      reader.onload = (e) => {\n        this.image2 = e.target.result\n        this.images.push(this.image2)\n        console.log('Images:   ',this.images)\n        }\n        \n      reader.readAsDataURL(file);\n    }, \n    removeImage2: function () {\n      this.image2 = '';\n    },\n\n\n     onFileChange3(e) {\n      var files = e.target.files || e.dataTransfer.files;\n      console.log(files)\n      if (!files.length)\n        return;\n      this.createImage3(files[0]);\n    },\n     createImage3(file) {\n       console.log(file)\n      var reader = new FileReader()\n\n      reader.onload = (e) => {\n        this.image3 = e.target.result\n        this.images.push(this.image3)\n        console.log('Images:   ',this.images)\n        }\n        \n      reader.readAsDataURL(file);\n    }, \n    removeImage3: function () {\n      this.image3 = '';\n    },\n\n\n     onFileChange4(e) {\n      var files = e.target.files || e.dataTransfer.files;\n      console.log(files[0])\n      if (!files.length)\n        return;\n      this.createImage4(files[0]);\n    },\n     createImage4(file) {\n       console.log(file)\n      var reader = new FileReader()\n\n      reader.onload = (e) => {\n        this.image4 = e.target.result\n        this.images.push(this.image4)\n        console.log('Images:   ',this.images)\n        }\n        \n      reader.readAsDataURL(file);\n    }, \n    removeImage4: function () {\n      this.image4 = '';\n    },\n\n\n     onFileChange5(e) {\n      var files = e.target.files || e.dataTransfer.files;\n      console.log(files)\n      if (!files.length)\n        return;\n      this.createImage5(files[0]);\n    },\n     createImage5(file) {\n       console.log(file)\n      var reader = new FileReader()\n\n      reader.onload = (e) => {\n        this.image5 = e.target.result\n        this.images.push(this.image5)\n        console.log('Images:   ',this.images)\n        }\n        \n      reader.readAsDataURL(file);\n    }, \n    removeImage5: function () {\n      this.image5 = '';\n    },\n\n    save(){\n        let product = {\n            name: this.productName,\n            color: this.productColor,\n            size: this.productSize,\n            description: this.productDescription,\n            categories: this.selectedCategories,\n            Price: this.productPrice,\n            quantity: this.quantity,\n            mainImage: this.mainImage,\n            images: this.images\n        }\n        this.$store.dispatch('saveProduct',product)\n    }\n  }\n}\n</script>\n<style  scoped>\n#app {\n  text-align: center;\n}\nimg {\n  width: 10%;\n  height: 70px;\n  margin: auto;\n  display: block;\n  margin-bottom: 10px;\n}\n.images-box{\n    margin-top: 20px;\n    padding: 10px;\n}\n.input{\n    width: 90%;\n    margin: 10px;\n}\n.category{\n    margin-bottom:50px;\n}\n\n\n</style>\n"]}]}